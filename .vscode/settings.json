{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "i18n-ally.localesPaths": [
        "messages",
        "src/i18n"
    ],
    "i18n-ally.sourceLanguage": "es",
    "i18n-ally.keystyle": "nested"
}
